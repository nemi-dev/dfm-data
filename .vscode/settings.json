{
  "json.schemas": [
    {
      "fileMatch": ["/data/item/**/*.json"],
      "url": "/schema/item.json"
    },
    {
      "fileMatch": ["/data/baseitem/**/*.json"],
      "url": "/schema/baseitem.json"
    },
    {
      "fileMatch": ["/data/itemset/**/*.json"],
      "url": "/schema/iset.json"
    },
    {
      "fileMatch": ["/data/dfclass/**/*.json"],
      "url": "/schema/dfclass.json"
    },
    {
      "fileMatch": ["/data/selfskill/**/*.json"],
      "url": "/schema/selfskill.json"
    },
    {
      "fileMatch": ["/data/skill/**/*.json"],
      "url": "/schema/attackskill.json"
    }
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "/dist/**/*": true
  },
  "python.analysis.typeCheckingMode": "off"
}